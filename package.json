{
  "name": "strapi-plugin-multi-tenant-v5",
  "version": "1.0.0",
  "description": "Turn Strapi 5 into a full-fledged backend for your SaaS. Multi-tenant support with Organizations and User Groups.",
  "license": "MIT",
  "strapi": {
    "name": "multi-tenant",
    "displayName": "Multi Tenant",
    "description": "Turn Strapi into a full-fledged backend for your SaaS.",
    "kind": "plugin"
  },
  "type": "commonjs",
  "exports": {
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "verify": "strapi-plugin verify"
  },
  "dependencies": {},
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.0.0",
    "@strapi/strapi": "^5.0.0",
    "@strapi/design-system": "^2.0.0",
    "@strapi/icons": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "author": {
    "name": "Felix Iim (based on work by Aneta JastrzÄ™bska)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felixlvh/strapi-plugin-multi-tenant-v5.git"
  },
  "keywords": [
    "strapi",
    "strapi-plugin",
    "multi-tenant",
    "saas",
    "organizations",
    "user-groups",
    "strapi-5"
  ],
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  }
}
